<h1>Dashboard</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Image</th>
    </tr>
  </thead>

  <tbody>
    <% current_user.posts.where(status: 'Approved').each do |p|%>
      <tr>
        <%= render 'post_show', p: p %>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Back', root_path, class: 'btn btn-default' %>
